<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    xmlns:app="http://schemas.android.com/apk/res-auto">
    <ImageView
        android:id="@+id/ivNotImg"
        android:layout_marginTop="@dimen/dip_110"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        android:layout_width="@dimen/dp_217"
        android:src="@mipmap/under_review_pic_ai"
        android:layout_height="@dimen/dp_217"/>
        <TextView
            android:layout_marginTop="@dimen/dp_35"
            android:text="您的申请正在审核中请耐心等待"
            android:gravity="center"
            android:paddingHorizontal="@dimen/dp_45"
            app:layout_constraintEnd_toEndOf="@+id/ivNotImg"
            app:layout_constraintStart_toStartOf="@+id/ivNotImg"
            app:layout_constraintTop_toBottomOf="@+id/ivNotImg"
            android:layout_width="@dimen/dp_0"
            android:textColor="@color/color_707070"
            android:textSize="@dimen/sp_14"
            android:layout_height="wrap_content"/>
</androidx.constraintlayout.widget.ConstraintLayout>